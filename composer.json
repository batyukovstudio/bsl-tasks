{
    "name": "batyukovstudio/bsl-tasks",
    "authors": [
        {
            "name": "example",
            "email": "example@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0",
        "batyukovstudio/bsl-core": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "BSLTasks\\App\\": "src/app/"
        }
    },
